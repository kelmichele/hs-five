<header class="header">
  <!-- <div id="top-bar"> -->
      <ul id="social-links">
          <li>
            <%= link_to '/fb', target: '_blank' do %>
              <i class="fa fa-facebook"></i>
            <% end %>
          </li>

          <li>
            <%= link_to '/insta', target: '_blank' do %>
              <i class="fa fa-instagram"></i>
            <% end %>
          </li>
      </ul>
  <!-- </div> -->

  <div class="main-header">
    <div class="col-md-3 logo-block">
      <%= link_to(image_tag('logo-block.png', alt: 'High Seas Design House, Logo', class: 'img-responsive logo-header'), root_path ) %>
    </div>

    <div class="col-md-9 mega-tab">
        <ul class="megamenu-parent menu-zoom hidden-xs">
            <%= render 'layouts/nav' %>
        </ul>
    </div>

    <div>
      <a href="#" id="offcanvas-toggler">
          <span></span>
          <span></span>
          <span></span>
      </a>
    </div>
</header>
